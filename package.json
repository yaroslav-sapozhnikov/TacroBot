{
   "name": "tacro-bot",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "ts-node": "3.3.0",
      "@types/node": "^8.0.29",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@vk-io/hear": "^1.1.0",
      "mongodb": "^3.0.8",
      "nodemon": "^2.0.15",
      "reflect-metadata": "^0.1.10",
      "typeorm": "^0.2.41",
      "typescript": "^4.5.3",
      "validator": "^13.7.0",
      "vk-io": "^4.4.0",
      "vk-io-question": "^3.1.1"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "nodemonConfig": {
      "ignore": [
         "**/*.test.ts",
         "**/*.spec.ts",
         ".git",
         "node_modules"
      ],
      "watch": [
         "src"
      ],
      "exec": "npm run start",
      "ext": "ts"
   }
}
